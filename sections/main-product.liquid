                      {%- endif -%}
                    </div>  

                    {%- if block.settings.show_quick_buy -%}
                      <div class="product-related-item__button">
                        {%- render 'quick-buy', product: product, button_classes: 'button--small' -%}
                      </div>
                    {%- endif -%}

                  </div>
                      
                {%- endfor -%}

              {%- endif -%}
            {% endcapture %}
          
            {%- render 'product-block', title: block.settings.heading, subtitle: block.settings.subtitle, text_color: block.settings.text_color, collapsible: block.settings.collapsible, background_color: block.settings.bg_color, darken_header: block.settings.darken_header, block_id: block.id, opened_by_default: block.settings.opened_by_default, image: block.settings.image, tooltip_info: block.settings.tooltip_info, layout: block.settings.layout, content: block_content -%}

          </product-recommendations>

        {%- when 'rating' -%}
          <div {{ block.shopify_attributes }}>
